{
  "name": "@nhnghia/assets",
  "version": "0.0.1",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "scripts": {
    "prepare": "npm run generate",
    "generate": "npm run generate-static-icons && npm run generate-illustrations && npm run generate-index-file",
    "generate-static-icons": "npx @svgr/cli --out-dir src/static-icons static-icons --silent",
    "generate-illustrations": "npx @svgr/cli --out-dir src/illustrations illustrations --silent",
    "generate-index-file": "sh ./_/generate-index-file.sh"
  },
  "peerDependencies": {
    "@svgr/cli": "^8.1.0"
  }
}
